function OpenerAssistant(argFromPusher) {
}

OpenerAssistant.prototype = {
	setup: function() {
		Ares.setupSceneAssistant(this);
	},
	cleanup: function() {
		Ares.cleanupSceneAssistant(this);
	},
	buttonTap: function(inSender, event) {
		this.controller.stageController.pushScene('main');
	}
};